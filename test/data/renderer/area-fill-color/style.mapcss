canvas {
    default-points: false;
    default-lines: false;
}

area[test=A] {
	fill-color: green;
	fill-opacity: 1;
}

area[test=B] {
	fill-color: rgba(0.5, 0, 1.0, 0.5);
	fill-opacity: 1;
}

area[test=C] {
	fill-color: red;
	fill-opacity: .5;
}

area[test=D] {
	fill-color: rgba(0.5, 0, 1.0, 0.5);
	fill-opacity: .5;
}

area[test=E] {
	fill-color: blue;
	fill-opacity: 1;
}

area[test=F] {
	fill-color: red;
	fill-opacity: 1;
}